{
  "name": "Pilares 2015 (Fiestas del Pilar)",
  "description": "Programa completo de las fiestas del pilar 2015. Te permite conectar con tus amigos y ver los eventos a los que van a ir.",
  "manifest_version": 2,
  "version": "0.0.4",
  "icons": {"128":"icon.png"},
  "offline_enabled": true,
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "Pilares 2015"
  },
  "permissions": [
    "https://ajax.googleapis.com/",
    "clipboardWrite",
    "storage",
    "http://*/",
    "http://*.google.com/",
    "http://localhost/*"
  ],
  "_app": {
    "background": {
      "scripts": ["background.js"]
    }
  }
}
